{
    "name": "Distract Block",
    "description": "Block distracting websites with ease!",
    "version": "1.0.0",
    "manifest_version": 2,
    "options_page": "options.html",
    "permissions": ["storage", "tabs", "http://*/*", "https://*/*"],
    "icons": {
        "128": "logo128.png",
        "32": "logo32.png",
        "24": "logo24.png",
        "16": "logo16.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": {
            "128": "logo128.png",
            "32": "logo32.png",
            "24": "logo24.png",
            "16": "logo16.png"
        },
        "default_title": "Distract Block",
        "default_popup": "popup.html"
    }
}
